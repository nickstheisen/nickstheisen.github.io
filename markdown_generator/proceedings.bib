@inproceedings{Theisen2019VDT,
   author = {Theisen, Nick and Paulus, Dietrich},
   editor = {Püschel, Frank and Stanke, Gerd and Pochanke, Michael},
   title = {Verbessern der temporalen {K}ohärenz in semantisch segmentierten, multispektralen {B}ildsequenzen},
   booktitle = {Tagungsband zum 24. Workshop Farbbildverarbeitung},
   organization = {Gesellschaft zur Förderung angewandter Informatik e.V.},
   address = {Berlin, Germany},
   volume = {24},
   pages = {75-85},
   month = {10},
   year = {2019},
   abstract = {Im Bereich des Rechnersehens spielt die semantische Bildsegmentierung eine zentrale Rolle. Sie erlaubt die Interpretation der Szene und der Objekte darin. Dies ermöglicht die Analyse und das logische Schlussfolgern auf einer höheren Abstraktionsebene. Besonders im Bereich der Robotik und der autonomen Navigation ist die genaue Kenntis des Umfelds eines Roboters entscheidend. Hier müssen oft ganze Folgen von Bildern segmentiert und klassifiziert werden, in denen sich häufig die Szene nur geringfügig verändert. Im Beitrag wird ein Verfahren vorgestellt, das auf Basis des optischen Flusses Korrespondenzen zwischen Pixeln aufeinanderfolgender Bilder erstellt. Diese Korrespondenzen werden zusammen mit den semantisch segmentierten Einzelbildern genutzt, um mit Hilfe eines einfachen Verfahrens eine temporal kohärente Segmentierung der Bildsequenz zu berechnen. Die Evaluation erfolgt auf Basis einer multispektralen Bildsequenz im Nahinfrarotbereich, deren Einzelbilder nach Vegetation segmentiert wurden. Es wird untersucht, ob das Verfahren geeignet, ist, eine höhere Konsistenz und somit eine bessere semantische Segmentierung zu erreichen.},
   isbn = {978-3-942709-23-1},
   url={https://www.researchgate.net/publication/338633765_Verbessern_der_temporalen_Koharenz_in_semantisch_segmentierten_multispektralen_Bildsequenzen},
}

